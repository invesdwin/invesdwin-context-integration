<?xml version="1.0" encoding="ISO-8859-1" ?>
<uddi>
	<client name="invesdwin">
		<nodes>
			<node>
				<name>default</name>
				<description>Main jUDDI node</description>
				<!-- JAX-WS Transport -->
				<proxyTransport>org.apache.juddi.v3.client.transport.JAXWSTransport
				</proxyTransport>
				<custodyTransferUrl>[REGISTRY_SERVER_URI]/custody-transfer
				</custodyTransferUrl>
				<inquiryUrl>[REGISTRY_SERVER_URI]/inquiry
				</inquiryUrl>
				<inquiryRESTUrl>[REGISTRY_SERVER_URI]/inquiryRest</inquiryRESTUrl>
				<publishUrl>[REGISTRY_SERVER_URI]/publish
				</publishUrl>
				<securityUrl>[REGISTRY_SERVER_URI]/security
				</securityUrl>
				<subscriptionUrl>[REGISTRY_SERVER_URI]/subscription
				</subscriptionUrl>
				<subscriptionListenerUrl>[REGISTRY_SERVER_URI]/subscription-listener
				</subscriptionListenerUrl>
				<juddiApiUrl>[REGISTRY_SERVER_URI]/juddi-api
				</juddiApiUrl>
			</node>
		</nodes>
	</client>
</uddi>